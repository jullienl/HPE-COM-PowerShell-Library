                                if ( $_.powerSupply.data){
                                    "{0}/{1}" -f ($_.powerSupply.data | ? PowerCapacityWatts -gt 0).count, $_.powerSupply.data.count
                                }
                                else {
                                    "0"
                                }
                                </ScriptBlock>
                            </TableColumnItem>                            
                            <TableColumnItem>
                                <ScriptBlock>
