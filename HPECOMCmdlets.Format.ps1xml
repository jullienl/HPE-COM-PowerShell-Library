                        <Label>ValidationStatus</Label>
                        <!-- <Width>10</Width> -->
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>ExpirationDate</Label>
                        <!-- <Width>10</Width> -->
                    </TableColumnHeader>
                </TableHeaders>
                <TableRowEntries>
                    <TableRowEntry>
                        <TableColumnItems>
                            <TableColumnItem>
                                <ScriptBlock>"{0} " -f $_.name</ScriptBlock>
                            </TableColumnItem>
                            <TableColumnItem>
                                <PropertyName>validatedByName</PropertyName>
                            </TableColumnItem>   
                            <TableColumnItem>
                                <PropertyName>validatedByEmail</PropertyName>
                            </TableColumnItem>     
                            <TableColumnItem>
                                <PropertyName>city</PropertyName>
                            </TableColumnItem> 
                            <TableColumnItem>
                                <PropertyName>country</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
                                <PropertyName>lat</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
                                <PropertyName>lng</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
                                <ScriptBlock>
                                    If ($_.validated -eq $true) {
                                        "Valid"
                                    }
                                    else {
                                        "Not Valid"
                                    }
                                </ScriptBlock>
                            </TableColumnItem>
                            <TableColumnItem>
                                <ScriptBlock>
                                    If ($_.validated -eq $true -and $_.validatedAt -and $_.validationCycle) {
                                        $expirationDate = ([DateTime]$_.validatedAt).AddMonths($_.validationCycle)
                                        $expirationDate.ToString('MM/dd/yyyy')
                                    }
                                    elseif ($_.expiredAt) {
                                        ([DateTime]$_.expiredAt).ToString('MM/dd/yyyy')
                                    }
                                    else {
                                        "N/A"
                                    }
                                </ScriptBlock>
                            </TableColumnItem>
                        </TableColumnItems>
                    </TableRowEntry>
                </TableRowEntries>
            </TableControl>
        </View>  

