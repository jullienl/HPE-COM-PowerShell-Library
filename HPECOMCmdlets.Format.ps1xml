        <View>
            <Name>Detailed Locations</Name>
            <ViewSelectedBy>
                <TypeName>HPEGreenLake.Location</TypeName>
            </ViewSelectedBy>
            <TableControl>
                <TableHeaders>
                    <TableColumnHeader>
                        <Label>Name</Label>
