                                        "{0}" -f $_.duration

                                    }
                                    elseif ($_.state -eq "Running") {
                                        "{0}" -f ((get-date) - (Get-date $_.createdAt.addHours($HPEGLGMTTimeDifferenceInHour))).ToString('hh\:mm\:ss')
