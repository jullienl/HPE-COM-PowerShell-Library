                                        $_.message.Substring(0, 57) + "..."
                                    } else {
                                        $_.message
                                    }
                                </ScriptBlock>
                            </TableColumnItem>
                            <TableColumnItem>
                                <PropertyName>workspaceName</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
                                <PropertyName>errorCode</PropertyName>
                            </TableColumnItem>
                        </TableColumnItems>
                    </TableRowEntry>
                </TableRowEntries>
            </TableControl>       
        </View> 

