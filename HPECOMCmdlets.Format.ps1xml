                                        "{0}/{1}" -f ($_.fan.data | ? Reading -gt 0).count, $_.fan.data.count
                                    }
                                    else {
                                        "0"
                                    }
                                </ScriptBlock>
                            </TableColumnItem>                            
                            <TableColumnItem>
                               <ScriptBlock>
                                    if ($_.smartUpdateToolInventory.data.ServiceState){
