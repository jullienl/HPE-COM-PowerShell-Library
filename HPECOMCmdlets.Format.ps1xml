        <View>
            <Name>Users Roles</Name>
            <ViewSelectedBy>
                <TypeName>HPEGreenLake.User.Role</TypeName>
            </ViewSelectedBy>
            <TableControl>
                <TableHeaders>
                    <TableColumnHeader>
                        <Label>Role</Label>
                        <!-- <Width>10</Width> -->
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>Service</Label>
                        <!-- <Width>10</Width> -->
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>Scope</Label>
                        <!-- <Width>10</Width> -->
                    </TableColumnHeader>
                     <TableColumnHeader>
                        <Label>Source of assignment</Label>
                        <!-- <Width>10</Width> -->
                    </TableColumnHeader>
                </TableHeaders>
                <TableRowEntries>
                    <TableRowEntry>
                        <TableColumnItems>
                            <TableColumnItem>
                                <PropertyName>role_name</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
                                <PropertyName>service_name</PropertyName>
                            </TableColumnItem>
                            <TableColumnItem>
                                <ScriptBlock>
                                    if ($_.scopes.scopeType -eq "MSP_SCOPE_GROUP" -or ($_.scopes -is [array] -and $_.scopes[0].scopeType -eq "MSP_SCOPE_GROUP")) {
                                        # Scope group assignment - show the scope group name(s)
                                        if ($_.scopes -is [array]) {
                                            # Multiple scope groups
                                            ($_.scopes | ForEach-Object { $_.scopeName }) -join ', '
                                        } elseif ($_.scopes.scopeName) {
                                            # Single scope group
                                            $_.scopes.scopeName
                                        } else {
                                            "Scope Group"
                                        }
                                    }
                                    elseif ($_.scopes.scopeType -contains "MSP_WORKSPACE" -or $_.scopes.scopeType -contains "WORKSPACE") { 
                                        "Entire Workspace" 
                                    }
                                    elseif ($_.scopes) {
                                        if ($_.scopes -is [array] -and $_.scopes.Count -gt 0) {
                                            if ($_.scopes[0].scopeName) {
                                                ($_.scopes | ForEach-Object { $_.scopeName }) -join ', '
                                            } else {
                                                $_.scopes -join ', '
                                            }
                                        } else {
                                            $_.scopes.ToString()
                                        }
                                    }
                                    else { 
                                        "Entire Workspace" 
                                    }
                                </ScriptBlock>
                            </TableColumnItem>
                            <TableColumnItem>
                                <PropertyName>source</PropertyName>
                            </TableColumnItem>
                        </TableColumnItems>
                    </TableRowEntry>
                </TableRowEntries>
            </TableControl>
        </View>

