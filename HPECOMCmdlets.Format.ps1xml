        <View>
            <Name>License</Name>
            <ViewSelectedBy>
                <TypeName>HPEGreenLake.License.AutoReassign</TypeName>
            </ViewSelectedBy>
            <TableControl>
                <TableHeaders>
                    <TableColumnHeader>
                        <Label>DeviceType</Label>
                        <!-- <Width>10</Width> -->
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>AutoReassignmentEnabled</Label>
                        <!-- <Width>10</Width> -->
                    </TableColumnHeader>
                </TableHeaders>
                <TableRowEntries>
                    <TableRowEntry>
                        <TableColumnItems>
                            <TableColumnItem>
                               <ScriptBlock>
                                    if ($_.deviceType -eq 'AP') {
                                        "Access Points"
                                    }
                                    elseif ($_.deviceType -eq 'SWITCH') {
                                        "Switches"
                                    }
                                    elseif ($_.deviceType -eq 'GATEWAY') {
                                        "Gateways"
                                    }
                                    elseif ($_.deviceType -eq 'COMPUTE') {
                                        "Compute"
                                    }
                                    elseif ($_.deviceType -eq 'SD_WAN_GW') {
                                        "DS-WAN Gateways"
                                    }
                                    elseif ($_.deviceType -eq 'SENSOR') {
                                        "Sensors"
                                    }
                                    elseif ($_.deviceType -eq 'BRIDGE') {
                                        "Bridges"
                                    }        
                                </ScriptBlock>
                            </TableColumnItem>
                            <TableColumnItem>
                               <PropertyName>enabled</PropertyName>
                            </TableColumnItem>
                        </TableColumnItems>
                    </TableRowEntry>
                </TableRowEntries>
            </TableControl>
        </View>

